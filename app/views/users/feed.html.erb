<h1><%= @user.username %>'s Feed</h1>

<table border="1">
  <tr>
    <th>Image</th>
    <th>Owner</th>
    <th>Caption</th>
    <th>Posted</th>
    <th>Likes</th>
    <th></th>
  </tr>
  
  <% if @feed_photos.present? %>
    <% @feed_photos.each do |photo| %>
      <tr>
        <td>
          <img src="<%= photo.image %>" class="img-responsive">
        </td>
        <td><%= photo.owner.username %></td>
        <td><%= photo.caption %></td>
        <td><%= time_ago_in_words(photo.created_at) %> ago</td>
        <td><%= photo.likes_count || photo.likes.count %></td>
        <td><%= link_to "Show details", photo_path(photo) %></td>
      </tr>
    <% end %>
  <% end %>
</table>
